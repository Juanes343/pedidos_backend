{
  "version": 2,
  "name": "pedidos-backend",
  "builds": [
    {
      "src": "api/servidor.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/servidor.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/servidor.js": {
      "maxDuration": 30
    }
  }
}